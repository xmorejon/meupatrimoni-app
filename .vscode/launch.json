{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:9002",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Next.js: Server",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal"
    }
  ]
}